<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd
		 http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.3.xsd">
    <changeSet id="2" author="MartynovDaniil">
        <createTable tableName="templates">
            <column name="id" type="int">
                <constraints primaryKey="true"/>
            </column>
            <column name="start_time" type="time"/>
            <column name="end_time" type="time"/>
            <column name="is_available" type="boolean"/>
        </createTable>
        <addAutoIncrement tableName="templates" columnName="id" columnDataType="int" startWith="1" incrementBy="1" />
    </changeSet>
</databaseChangeLog>